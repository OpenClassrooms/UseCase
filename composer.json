{
    "name": "openclassrooms/use-case",
    "description": "Use case library",
    "type": "library",
    "keywords": ["design"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Kuzniak",
            "email": "romain.kuzniak@turn-it-up.org"
        },
        {
            "name": "OpenClassrooms",
            "email": "tech@openclassrooms.com"
        }
